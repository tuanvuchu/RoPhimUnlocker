{
  "manifest_version": 3,
  "name": "RoPhim Unlocker",
  "version": "1.0.0",
  "description": "Mở khóa tính năng VIP và coin không giới hạn cho RoPhim.",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "key": "",
  "content_scripts": [
    {
      "matches": ["https://www.rophim.li/*"],
      "js": ["assets/content.js"],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ],
  "host_permissions": ["https://www.rophim.li/*"]
}
